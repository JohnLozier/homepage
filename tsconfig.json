{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"explainFiles": true,
		"jsx": "preserve",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"types": [
			"vite/client"
		]
	}
}