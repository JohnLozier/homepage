{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"target": "ESNext",
		"module": "esnext",
		"explainFiles": true,
		"jsx": "preserve",
		"moduleResolution": "bundler",
		"jsxImportSource": "solid-js",
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"verbatimModuleSyntax": null,
		"isolatedModules": true,
		"paths": {
			"~/*": ["./src/*"]
		},
		"baseUrl": "."
	}
}